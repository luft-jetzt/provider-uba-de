{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "autumndev/jms-serializer-carbon": "^3.3",
        "jms/serializer-bundle": "^5.4.0",
        "luft-jetzt/luft-api-bundle": "^0.8.1",
        "luft-jetzt/luft-model": "^0.4",
        "nesbot/carbon": "^3.6.0",
        "symfony/console": "^7.1",
        "symfony/dotenv": "^7.1",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^7.1",
        "symfony/runtime": "^7.1",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.23"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.1"
        }
    }
}
